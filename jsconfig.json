{
  "compilerOptions": {
    "target": "ES6",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "module": "commonjs",
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "@acore/noderouter/*" : [
        "*"
      ],
      "/*" : [
        "./src/*"
      ]
    }
  },
  "include": ["src"]
}
